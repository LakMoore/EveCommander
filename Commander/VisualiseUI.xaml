<Window x:Class="Commander.VisualiseUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Commander"
        mc:Ignorable="d"
        Title="VisualiseUI" Height="450" Width="800"
        Background="{StaticResource BackgroundBrush}" Foreground="{StaticResource ForegroundBrush}">
  <Grid>
    <Grid x:Name="EveRoot" Background="{StaticResource SurfaceBrush}" />
    <TextBlock x:Name="Path" Text="VisualiseUI" HorizontalAlignment="Left" Width="200" Margin="15" TextWrapping="Wrap" Foreground="{StaticResource SecondaryForegroundBrush}"/>
  </Grid>
</Window>
